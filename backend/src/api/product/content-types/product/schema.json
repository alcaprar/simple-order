{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "shop": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::shop.shop",
      "inversedBy": "products"
    },
    "unit": {
      "type": "enumeration",
      "enum": [
        "Hg",
        "Kg",
        "Piece"
      ]
    },
    "product_sales": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-sale.product-sale",
      "mappedBy": "product"
    }
  }
}
